@using eMotive.Models.Objects.Pages
@model PartialPage   
@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
@section Crumbs
{
    			<ul class="breadcrumb">
			        <li>
			            <a href="@Url.Action("Index","Home", new {area = "Admin"})">Home</a> <span class="divider">/</span>
			        </li>
                <li>
					<a href="@Url.Action("Index","Pages", new {area = "Admin"})">Pages</a> <span class="divider">/</span>
				</li>
				<li class="active">
					Create Partial Page
				</li>
			</ul>
}
<div class="page-header">
    <h2>
        Create Partial Page
    </h2>
</div>
@Html.Partial("_modelStateError")
@using (Html.BeginForm("Create", "Pages", new { area = "Admin" }, FormMethod.Post, new { @class = "form-horizontal" }))
{
     <div class="control-group">
        <label class="control-label" for="Key">Key</label>
        <div class="controls">
            @Html.TextBoxFor(n => n.Key)
        </div>
    </div>
    @Html.Partial("_partialPageForm")
}
@section Scripts
{
    @Html.Partial("_tinymceEditor")
}