  <system.web>

    <httpRuntime targetFramework="4.5" />
    <compilation debug="true" targetFramework="4.5.1">

      <buildProviders>
        <remove extension=".edmx" />
      </buildProviders>
    </compilation>
    <httpHandlers>
      <add path="api" type="ServiceStack.WebHost.Endpoints.ServiceStackHttpHandlerFactory,ServiceStack" verb="*" />
      <add verb="*" type="eMotive.SCE.Core.Handlers.RoxyFilemanHandler, eMotive.SCE, Version=1.0.0.0, Culture=neutral" path="FileManager" />
    </httpHandlers>
    <authentication mode="None">
      <forms loginUrl="~/Account/Login" timeout="20" />
    </authentication>
    <roleManager enabled="true" defaultProvider="eMotiveRoleProvider" cacheRolesInCookie="true">
      <providers>
        <clear />
        <add name="eMotiveRoleProvider" type="eMotive.SCE.Common.Providers.eMotiveRoleProvider, eMotive.SCE" />
      </providers>
    </roleManager>
    <pages>
